{
  "extends": [
    "next/core-web-vitals", 
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    // Disable the specific rule
    "@typescript-eslint/no-unused-vars": "off",

    // You can also set a rule to "warn" or "error"
